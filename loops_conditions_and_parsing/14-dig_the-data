#!/usr/bin/env bash
# This script checks for the existence of the Apache log file and outputs an error if not found

# Define the log file
log_file="access.log"

# Check if the log file exists
if [[ ! -f "$log_file" ]]; then
  echo "Log file not found!"
  exit 1
fi

